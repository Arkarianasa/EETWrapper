<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
	</startup>
	<system.diagnostics>
		<sources>
			<source name="System.ServiceModel"
              switchValue="Information, ActivityTracing"
              propagateActivity="true">
				<listeners>
					<add name="traceListener"
              type="System.Diagnostics.XmlWriterTraceListener"
              initializeData="C:\temp\traces.svclog"  />
				</listeners>
			</source>
		</sources>
	</system.diagnostics>
	<system.serviceModel>
		<bindings>
			<customBinding>
				<binding name="EETSOAP">
					<transactionFlow />
					<security authenticationMode="CertificateOverTransport"  defaultAlgorithmSuite="Basic256Sha256Rsa15" 
										messageSecurityVersion="WSSecurity11WSTrustFebruary2005WSSecureConversationFebruary2005WSSecurityPolicy11BasicSecurityProfile10" 
										requireDerivedKeys="false" enableUnsecuredResponse="true" securityHeaderLayout="Lax" />
					<textMessageEncoding  messageVersion="Soap12"/>
					<httpsTransport />
				</binding>
			</customBinding>

			<wsHttpBinding>
				<binding name="EETSOAP">
					<security mode="TransportWithMessageCredential">
						<transport clientCredentialType="Certificate"/>
						<message clientCredentialType="Certificate" establishSecurityContext="false" algorithmSuite="Basic256Sha256Rsa15" negotiateServiceCredential="False"/>
					</security>
				</binding>
			</wsHttpBinding>
			<basicHttpsBinding>
				<binding name="EETSOAP">
					<security mode="TransportWithMessageCredential" >
						<transport clientCredentialType="Certificate"/>
						<message algorithmSuite="Basic256Sha256Rsa15" clientCredentialType="Certificate"/>
					</security>
				</binding>
			</basicHttpsBinding>
		</bindings>
		<behaviors>
			<endpointBehaviors>
				<behavior name="signature">
					<clientCredentials >
						<clientCertificate storeLocation="CurrentUser" storeName="My" findValue="CZ00000019" x509FindType="FindBySubjectName"/>						
					</clientCredentials>
				</behavior>
			</endpointBehaviors>
		</behaviors>
		<client>
			<endpoint address="https://pg.eet.cz:443/eet/services/EETServiceSOAP/v3"
        binding="customBinding" bindingConfiguration="EETSOAP" contract="EETService_v3.EET" behaviorConfiguration="signature"
        name="EETServiceSOAP" />
		</client>
	</system.serviceModel>
</configuration>